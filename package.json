  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": {
    "name": "mayank.harsani",
    "email": "mayank.harsani@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psychobunny/nodebb-plugin-quickstart/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.15.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-angular": "12.0.1",
    "eslint": "7.22.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "husky": "5.1.3",
    "lint-staged": "10.5.4"
  },
  "dependencies": {
    "node-umeng": "^1.0.2"
  }
