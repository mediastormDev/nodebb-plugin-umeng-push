{
  "name": "nodebb-plugin-umeng-push",
  "version": "1.0.0",
  "description": "A plugin to send notifications to a particular url",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/psychobunny/nodebb-plugin-quickstart"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "firebase",
    "url",
    "notification"
  ],
  "husky": {
